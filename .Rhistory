df
library(ggplot2)
ggplot(df, aes(x=pais, y=b37r)) +
geom_bar(stat = "identity")
ggplot(df, aes(x=pais, y=b37r)) +
geom_bar(stat = "identity") + coord_flip() #Coord_flicp sirve para voltear el gráfico de manera horizontal
ggplot(df, aes(x=reorder(pais,b37r), y=b37r)) +
geom_bar(stat = "identity") +
coord_flip()+
labs(title = "Confianza en los medios en las Américas",
subtitle = "Barómetro de las Américas",
x = " ",
y = "Escala 0 - 100")
#Reorder sirve para solicitar que se reorganize respecto a la variable que se selecciona
