geom_bar(stat = "identity") +
geom_errorbar(aes(ymin=b37r-ci, ymax=b37r+ci), width= 0.2) + #ci es el intervalo de confianza que ya se ha encontrado
coord_flip()+
labs(title = "Confianza en los medios en las Américas",
